ssage.error(error.response.data.msg);\n      })\n    },\n    handleAvatarSuccess(res) {\n      this.form.avatar = res.data;\n    }\n  }\n}\n</script>\n\n<style>\n.avatar-uploader .el-upload {\n    border: 1px dashed #d9d9d9;\n    cursor: pointer;\n    position: relative;\n    overflow: hidden;\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409EFF;\n}\n.avatar-uploader-icon {\n  font-size: 60px;\n  color: #8c939d;\n  width: 178px;\n  height: 178px;\n  line-height: 100%;\n  text-align: center;\n}\n.avatar {\n  width: 178px;\n  height: 178px;\n  display: block;\n}\n</style>\n"],"mappings":"AAwCA,SAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;QACAC,aAAA,EAAAJ,QAAA;MACA;MACAK,IAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA,GAAAC,IAAA,CAAAC,GAAA;MACA,KAAAL,IAAA,GAAAK,GAAA,CAAAR,IAAA;IACA,GAAAS,KAAA,CAAAC,KAAA;MACA,KAAAC,QAAA,CAAAD,KAAA,CAAAA,KAAA,CAAAE,QAAA,CAAAZ,IAAA,CAAAa,GAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAR,QAAA;MACA,mBAAAS,KAAA,CAAAC,GAAA,iBAAAhB,IAAA;IACA;IACAiB,KAAA;MACA,KAAAF,KAAA,CAAAG,IAAA,oBAAAf,IAAA,EAAAI,IAAA,CAAAC,GAAA;QACA,IAAAW,MAAA,GAAAX,GAAA,CAAAR,IAAA;QACA,IAAAmB,MAAA,CAAAC,IAAA;UACA,KAAAT,QAAA,CAAAU,OAAA;UACA,KAAAC,KAAA;UACA,KAAAhB,OAAA,GAAAC,IAAA,CAAAC,GAAA;YACA,KAAAL,IAAA,GAAAK,GAAA,CAAAR,IAAA;UACA;QACA;UACA,KAAAW,QAAA,CAAAD,KAAA;QACA;MACA,GAAAD,KAAA,CAAAC,KAAA;QACA,KAAAC,QAAA,CAAAD,KAAA,CAAAA,KAAA,CAAAE,QAAA,CAAAZ,IAAA,CAAAa,GAAA;MACA;IACA;IACAU,oBAAAf,GAAA;MACA,KAAAL,IAAA,CAAAqB,MAAA,GAAAhB,GAAA,CAAAR,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
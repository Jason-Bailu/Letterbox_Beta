{"ast":null,"code":"import { getToken } from '@/utils/auth';\nexport default {\n  name: \"AdminView\",\n  data() {\n    return {\n      token: {\n        Authorization: getToken()\n      },\n      form: {},\n      user: {}\n    };\n  },\n  created() {\n    this.getUser().then(res => {\n      this.form = res.data;\n    }).catch(error => {\n      this.$message.error(error.response.data.msg);\n    });\n  },\n  methods: {\n    async getUser() {\n      return (await this.axios.get(\"/user/local\")).data;\n    },\n    save() {\n      this.axios.post(\"/user/save\", this.form).then(res => {\n        let result = res.data;\n        if (result.code === 'C200') {\n          this.$message.success(\"保存成功\");\n          this.$emit(\"refreshUser\");\n          this.getUser().then(res => {\n            this.form = res.data;\n          });\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      }).catch(error => {\n        this.$message.error(error.response.data.msg);\n      });\n    },\n    handleAvatarSuccess(res) {\n      this.form.avatar = res.data;\n    }\n  }\n};","map":{"version":3,"names":["getToken","name","data","token","Authorization","form","user","created","getUser","then","res","catch","error","$message","response","msg","methods","axios","get","save","post","result","code","success","$emit","handleAvatarSuccess","avatar"],"sources":["src/views/systemview/AdminView.vue"],"sourcesContent":["<template>\n  <div>\n    <el-card style=\"width: 500px; margin: auto; margin-top: 40px;\">\n      <el-form :model=\"form\" label-width=\"100px\" size=\"small\">\n        <el-upload\n            class=\"avatar-uploader\"\n            action=\"http://localhost:9090/letterbox-api/file/upload\"\n            :headers=\"token\"\n            :show-file-list=\"false\"\n            :on-success=\"handleAvatarSuccess\">\n          <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\">\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n       
lumn>\n    </el-table>\n    <div style=\"padding: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-size=\"pageSize\"\n          :page-sizes=\"[2, 5, 10, 15]\"\n          :total=\"total\"\n          layout=\"total, sizes, prev, pager, next, jumper\">\n      </el-pagination>\n    </div>\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n      <el-form :model=\"form\" label-width=\"100px\" size=\"small\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" disabled autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"昵称\">\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"邮件\">\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机\">\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"地址\">\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"UserView\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      form: {},\n      dialogFormVisible: false\n    }\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.axios.get(\"/user/page\",{\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.total = result.data.total;\n          this.tableData = result.data.page;\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    handleEdit(user) {\n      this.form = Object.assign({}, user);\n      this.dialogFormVisible = true;\n    },\n    save() {\n      this.axios.post(\"/user/save\", this.form).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    handleDelete(id) {\n      this.axios.delete(\"/user/delete/\"+id).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":"AAwEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,IAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,KAAA,CAAAC,GAAA;QACAC,MAAA;UACAV,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAU,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAd,IAAA;QACA,IAAAe,MAAA,CAAAC,IAAA;UACA,KAAAZ,KAAA,GAAAW,MAAA,CAAAf,IAAA,CAAAI,KAAA;UACA,KAAAH,SAAA,GAAAc,MAAA,CAAAf,IAAA,CAAAiB,IAAA;QACA;UACA,KAAAC,QAAA,CAAAC,KAAA,CAAAJ,MAAA,CAAAK,GAAA;QACA;MACA;IACA;IACAC,WAAAC,IAAA;MACA,KAAAjB,IAAA,GAAAkB,MAAA,CAAAC,MAAA,KAAAF,IAAA;MACA,KAAAhB,iBAAA;IACA;IACAmB,KAAA;MACA,KAAAf,KAAA,CAAAgB,IAAA,oBAAArB,IAAA,EAAAQ
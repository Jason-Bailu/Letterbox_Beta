{"ast":null,"code":"export default {\n  name: \"UserView\",\n  data() {\n    return {\n      tableData: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      form: {},\n      dialogFormVisible: false\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.axios.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.total = result.data.total;\n          this.tableData = result.data.page;\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    handleEdit(user) {\n      this.form = Object.assign({}, user);\n      this.dialogFormVisible = true;\n    },\n    save() {\n      this.axios.post(\"/user/save\", this.form).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    handleDelete(id) {\n      this.axios.delete(\"/user/delete/\" + id).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","pageNum","pageSize","total","form","dialogFormVisible","created","load","methods","axios","get","params","then","res","result","code","page","$message","error","msg","handleEdit","user","Object
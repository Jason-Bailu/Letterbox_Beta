{"ast":null,"code":"export default {\n  name: \"MenuView\"\n};","map":{"version":3,"names":["name"],"sources":["src/views/systemview/MenuView.vue"],"sourcesContent":["<template>\n  <div>\n    <el-table\n        :data=\"tableData\"\n        height=\"60vh\"\n        style=\"width: 100%; border-radius: 10px\"\n        border stripe\n        \n        @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\"/>\n      <el-table-column prop=\"id\" label=\"ID\" width=\"55\"/>\n      <el-table-column prop=\"menuName\" label=\"菜单名\"/>\n      <el-table-column prop=\"path\" label=\"路径\"/>\n      <el-table-column prop=\"component\" label=\"组件\"/>\n      <el-table-column label=\"图标\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <i :class=\"scope.row.icon\" style=\"font-size: 20px\"/>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"discribe\" label=\"描述\"/>\n      <el-table-column prop=\"option\" label=\"操作\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm\n              confirm-button-text='好的'\n              cancel-button-text='不用了'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"确定删除吗？\"\n              @confirm=\"handleDelete(scope.row.id)\">\n            <el-button type=\"danger\" slot=\"reference\" style=\"margin-left: 5px\">删除</el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"padding: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-size=\"pageSize\"\n          :page-sizes=\"[2, 5, 10, 15]\"\n          :total=\"total\"\n          layout=\"total, sizes, prev, pager, next, jumper\">\n      </el-pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"MenuView\"\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAkDA;EACAA,IAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
              params: {\n                   pageNum: this.pageNum,\n                   pageSize: this.pageSize \n                }\n            }).then(res => {\n                let result = res.data;\n                if (result.code === 'C200') {\n                    this.tableData = result.data.page;\n                    this.total = result.data.total;\n                } else {\n                    this.$message.error(result.msg);\n                }\n            })\n        },\n        handleChange(value) {\n            console.log(value);\n        },\n        handleSelectionChange(value) {\n            this.multipleSelection = value;\n        },\n        handleSizeChange(pageSize) {\n            this.pageSize = pageSize;\n            this.load();\n        },\n        handleCurrentChange(pageNum) {\n            this.pageNum = pageNum;\n            this.load();\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAsFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,YAAA;MACAC,YAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,iBAAA;MACAC,aAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,KAAA,CAAAC,GAAA,kBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAf,IAAA;QACA,IAAAgB,MAAA,CAAAC,IAAA;UACA,KAAAhB,YAAA,GAAAe,MAAA,CAAAhB,IAAA;QACA;UACA,KAAAkB,QAAA,CAAAC,KAAA,CAAAH,MAAA,CAAAI,GAAA;QACA;MACA;MACA,KAAAR,KAAA,CAAAC,GAAA;QACAQ,MAAA;UACAjB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAf,IAAA;QACA,IAAAgB,MAAA,CAAAC,IAAA;UACA,KAAAd,SAAA,GAAAa,MAAA,CAAAhB,IAAA,CAAAsB,IAAA;UACA,KAAAhB,KAAA,GAAAU,MAAA,CAAAhB,IAAA,CAAAM,KAAA;QACA;UACA,KAAAY,QAAA,CAAAC,KAAA,CAAAH,MAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAG,YAAAC,GAAA;MACA,KAAAtB,YAAA,GAAAsB,GAAA,CAAAC,SAAA;MACA,KAAAb,KAAA,CAAAC,GAAA,sBAAAX,YAAA;QACAmB,MAAA;UACAjB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAf,IAAA;QACA,IAAAgB,MAAA,CAAAC,IAAA;UACA,KAAAd,SAAA,GAAAa,MAAA,CAAAhB,IAAA,CAAAsB,IAAA;UACA,
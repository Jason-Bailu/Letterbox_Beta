{"ast":null,"code":"export default {\n  name: \"UserView\",\n  data() {\n    return {\n      username: \"\",\n      nickname: \"\",\n      address: \"\",\n      tableData: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.axios.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.total = result.data.total;\n          this.tableData = result.data.page;\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    search() {\n      this.axios.get(\"/user/find\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          nickname: this.nickname,\n          address: this.address\n        }\n      }).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.total = result.data.total;\n          this.tableData = result.data.page;\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    reset() {\n      this.total = 0;\n      this.pageNum = 1;\n      this.pageSize = 5;\n      this.username = \"\";\n      this.nickname = \"\";\n      this.address = \"\";\n      this.load();\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      this.axios.post(\"/user/batchDelete\", ids).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    handleEdit(user) {\n      this.form = Object.assign({}, user);\n      this.dialogFormVisible = true;\n    },\n    save() {\n      this.axios.post(\"/user/save\", this.form).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    handleDelete(id) {\n      this.axios.delete(\"/user/delete/\" + id).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","username","nickname","address","tableData","pageNum","pageSize","total","form","dialogFormVisible","multipleSelection","created","load","methods","axios","get","params","then","res","result","code","page","$message","error","msg","search","reset","handleAdd","handleSelectionChange","val","console","log","delBatch","ids","map","v","id","post","success","handleEdit","user","Object","assign","save","handleDelete","delete","handleSizeChange","handleCurrentChange"],"sources":["src/views/systemview/UserView.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin-bottom: 10px; height: 60px; line-height: 60px\">\n      <el-input style=\"width: 200px; margin-left: 5px\" suffix-icon=\"el-icon-search\" placeholder=\"请输入名称\" v-model=\"username\"></el-input>\n      <el-input style=\"width: 200px; margin-left: 5px\" suffix-icon=\"el-icon-message\" placeholder=\"请输入昵称\" v-model=\"nickname\"></el-input>\n      <el-input style=\"width: 200px; margin-left: 5px\" suffix-icon=\"el-icon-position\" placeholder=\"请输入地址\" v-model=\"address\"></el-input>\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"search\">搜索</el-button>\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\">重置</el-button>\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\" style=\"margin-left: 5px\"></i></el-button>\n      <el-popconfirm\n          confirm-button-text='好的'\n          cancel-button-text='不用了'\n          icon=\"el-icon-info\"\n          icon-color=\"red\"\n          title=\"确定批量删除吗？\"\n          @confirm=\"delBatch\">\n        <el-button style=\"margin-left: 5px\" type=\"danger\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\" style=\"margin-left: 5px\"></i></el-button>\n      </el-popconfirm>\n    </div>\n    <el-table\n        :data=\"tableData\"\n        style=\"width: 100%; align-content: center; border-radius: 10px\" @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\"/>\n      <el-table-column prop=\"id\" label=\"ID\"/>\n      <el-table-column prop=\"username\" label=\"用户名\"/>\n      <el-table-column prop=\"nickname\" label=\"昵称\"/>\n      <el-table-column prop=\"email\" label=\"邮箱\"/>\n      <el-table-column prop=\"phone\" label=\"手机\"/>\n      <el-table-column prop=\"address\" label=\"地址\"/>\n      <el-table-column prop=\"avatar\" label=\"头像\"/>\n      <el-table-column prop=\"option\" label=\"操作\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm\n              confirm-button-text='好的'\n              cancel-button-text='不用了'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"确定删除吗？\"\n              @confirm=\"handleDelete(scope.row.id)\">\n            <el-button type=\"danger\" slot=\"reference\" style=\"margin-left: 5px\">删除</el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"padding: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-size=\"pageSize\"\n          :page-sizes=\"[2, 5, 10, 15]\"\n          :total=\"total\"\n          layout=\"total, sizes, prev, pager, next, jumper\">\n      </el-pagination>\n    </div>\n    <el-dialog custom-class=\"user-dialog\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n      <div slot=\"title\" style=\"height: 60px; line-height: 60px\">用户信息</div>\n      <el-form :model=\"form\" label-width=\"100px\" size=\"small\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"昵称\">\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"邮件\">\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机\">\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"地址\">\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\" style=\"height: 70px; line-height: 70px\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"UserView\",\n  data() {\n    return {\n      username: \"\",\n      nickname: \"\",\n      address: \"\",\n      tableData: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: []\n    }\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.axios.get(\"/user/page\",{\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.total = result.data.total;\n          this.tableData = result.data.page;\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    search() {\n      this.axios.get(\"/user/find\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          nickname: this.nickname,\n          address: this.address\n        }\n      }).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.total = result.data.total;\n          this.tableData = result.data.page;\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    reset() {\n      this.total = 0;\n      this.pageNum = 1;\n      this.pageSize = 5;\n      this.username = \"\";\n      this.nickname = \"\";\n      this.address = \"\";\n      this.load();\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      this.axios.post(\"/user/batchDelete\", ids).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      })\n    },\n    handleEdit(user) {\n      this.form = Object.assign({}, user);\n      this.dialogFormVisible = true;\n    },\n    save() {\n      this.axios.post(\"/user/save\", this.form).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    handleDelete(id) {\n      this.axios.delete(\"/user/delete/\"+id).then(res => {\n        let result = res.data;\n        if (result.code === '200') {\n          this.$message.success(result.msg);\n          this.load();\n        } else {\n          this.$message.error(result.msg);\n        }\n      })\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n}\n</script>\n\n<style>\n.user-dialog {\n  border-radius: 20px;\n}\n</style>\n"],"mappings":"AAoFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;MACAC,OAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,KAAA,CAAAC,GAAA;QACAC,MAAA;UACAX,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAW,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAlB,IAAA;QACA,IAAAmB,MAAA,CAAAC,IAAA;UACA,KAAAb,KAAA,GAAAY,MAAA,CAAAnB,IAAA,CAAAO,KAAA;UACA,KAAAH,SAAA,GAAAe,MAAA,CAAAnB,IAAA,CAAAqB,IAAA;QACA;UACA,KAAAC,QAAA,CAAAC,KAAA,CAAAJ,MAAA,CAAAK,GAAA;QACA;MACA;IACA;IACAC,OAAA;MACA,KAAAX,KAAA,CAAAC,GAAA;QACAC,MAAA;UACAX,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAL,QAAA,OAAAA,QAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,OAAA,OAAAA;QACA;MACA,GAAAc,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAlB,IAAA;QACA,IAAAmB,MAAA,CAAAC,IAAA;UACA,KAAAb,KAAA,GAAAY,MAAA,
r(result.msg);\n                }\n            })\n        },\n        handleCheck(row) {\n            this.categoryName = row.className;\n            this.axios.get(\"/good/find/\"+this.categoryName, {\n                params: {\n                   pageNum: this.pageNum,\n                   pageSize: this.pageSize \n                }\n            }).then(res => {\n                let result = res.data;\n                if (result.code === 'C200') {\n                    this.tableData = result.data.page;\n                    this.total = result.data.total;\n                } else {\n                    this.$message.error(result.msg);\n                }\n            })\n        },\n        handleChange(value) {\n            console.log(value);\n        },\n        handleSelectionChange(value) {\n            this.multipleSelection = value;\n        },\n        handleAdd(row) {\n            this.dialogVisible = true;\n            this.form = {};\n        },\n        handleEdit(row) {\n            this.dialogVisible = true;\n            this.form = row;\n        },\n        handleSizeChange(pageSize) {\n            this.pageSize = pageSize;\n            this.load();\n        },\n        handleCurrentChange(pageNum) {\n            this.pageNum = pageNum;\n            this.load();\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"],"mappings":"AAsHA,SAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,YAAA;MACAC,YAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,iBAAA;MACAC,aAAA;MACAC,IAAA;MACAC,KAAA;QACAC,aAAA,EAAAb,QAAA;MACA;IACA;EACA;EACAc,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,KAAA,CAAAC,GAAA,kBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAlB,IAAA;QACA,IAAAmB,MAAA,CAAAC,IAAA;UACA,KAAAnB,YAAA,GAAAkB,MAAA,CAAAnB,IAAA;QACA;UACA,KAAAqB,QAAA,CAAAC,KAAA,CAAAH,MAAA,CAAAI,GAAA;QACA;MACA;MACA,KAAAR,KAAA,CAAAC,GAAA;QACAQ,MAAA;UACApB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAY,IAAA,CAAAC,GAAA;QACA,IAAAC,MAAA,GAAAD,GAAA,CAAAlB,IAAA;QACA,IAAAmB